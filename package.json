{
  "name": "formik-project",
  "private": true,
  "type": "module",
  "packageManager": "yarn@1.22.22",
  "resolutions": {
    "shelljs": "0.8.5",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@hookform/resolvers": "^3.1.1",
    "@playwright/test": "^1.34.3",
    "@types/benchmark": "^2.1.2",
    "@types/jest": "^25.0.0",
    "benchmark": "^2.1.4",
    "esm": "^3.2.25",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "react-hook-form": "^7.45.0",
    "tslib": "^2.5.3",
    "tsx": "^4.19.0"
  },
  "workspaces": [
    "packages/*",
    "website"
  ],
  "scripts": {
    "dev": "npm run start --workspace=packages/formik",
    "test": "npm run test --workspaces --if-present",
    "build": "npm run build --workspace=packages/formik",
    "build:formik": "npm run build --workspace=packages/formik",
    "build:all": "npm run build --workspaces --if-present",
    "prepublish": "npm run build --workspace=packages/formik",
    "format": "prettier --write 'examples/**/*' 'packages/*/src/**/*' 'website/src/**/*.{ts,tsx,js,jsx,md,mdx}' 'app/pages/**/*'  'packages/*/test/**/*' 'README.md'",
    "changeset": "changeset",
    "release": "changeset publish",
    "git-reset": "git reset --hard HEAD",
    "git-clean": "git clean -d -x -e node_modules -e packages -f",
    "e2e": "playwright test",
    "precommit": "lint-staged",
    "e2e:ui": "playwright test --ui",
    "start:app": "npm run build --workspace=packages/formik && npm --prefix packages/formik link && npm --prefix ./app link formik && npm --prefix ./app run dev",
    "benchmark": "tsx scripts/benchmark.tsx | tee output.txt"
  },
  "lint-staged": {
    "**/*.{ts,tsx,md,mdx,js,jsx}": [
      "npm run format",
      "tsdx lint"
    ]
  }
}
